{
  "name": "typescript-starter",
  "version": "2.2.2",
  "description": "TypeScript starter",
  "license": "Proprietary",
  "scripts": {
    "dev": "npm run clean && NODE_ENV=development LOG_LEVEL=debug tsc-watch --preserveWatchOutput -w --noEmitOnError false --onSuccess \"node -r module-alias/register -r source-map-support/register ./build/src/app.js\"",
    "dev:debug": "npm run clean && NODE_ENV=development LOG_LEVEL=debug tsc-watch --preserveWatchOutput -w --noEmitOnError false --onSuccess \"node -r module-alias/register -r source-map-support/register --inspect-brk=0.0.0.0:9222 ./build/src/app.js\"",
    "test": "NODE_ENV=testing jest -c .jest-unit.json --watchAll",
    "test:debug": "NODE_ENV=testing node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest -c .jest-unit.json --runInBand",
    "test:once": "NODE_ENV=testing jest -c .jest-unit.json --coverage",
    "posttest:once": "npm run check",
    "e2e": "NODE_ENV=e2e jest -c .jest-e2e.json --watchAll",
    "e2e:debug": "NODE_ENV=e2e node --inspect-brk=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest -c .jest-e2e.json --runInBand",
    "e2e:once": "NODE_ENV=e2e jest -c .jest-e2e.json",
    "check": "eslint -c .eslintrc.fix.json '{src,test}/**/*.{ts,js}'",
    "fix": "eslint -c .eslintrc.fix.json --fix '{src,test}/**/*.{ts,js}'",
    "clean": "rm -rf build/",
    "compile": "echo Compiling... && tsc -p .",
    "compile:staging": "npm run compile:production",
    "compile:production": "npm run clean && echo Compiling... && tsc -p tsconfig.build.json",
    "start": "node -r module-alias/register build/src/app.js",
    "start:develop": "NODE_ENV=staging LOG_LEVEL=debug npm run start -- -r source-map-support/register",
    "start:staging": "NODE_ENV=staging npm run start",
    "start:production": "NODE_ENV=production npm run start",
    "shell:build": "docker-compose -f docker-compose.shell.yml build --no-cache",
    "shell": "docker-compose -f docker-compose.shell.yml build && docker-compose -f docker-compose.shell.yml run --rm -u 1000 --name=typescript-starter-CHANGEME --service-ports app bash || true && echo Stopping environment... && npm run shell:stop",
    "shell:join": "docker exec -it typescript-starter-CHANGEME bash",
    "shell:stop": "docker-compose -f docker-compose.shell.yml down",
    "precommit": "lint-staged"
  },
  "_moduleAliases": {
    "src": "build/src/"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@elunic/ecs-commitlint": "^0.1.1",
    "@elunic/ecs-prettier": "^0.2.1",
    "@elunic/ecs-tsconfig": "^0.1.1",
    "@elunic/eslint-config-ecs": "^0.1.10",
    "@nestjs/testing": "^7.6.1",
    "@types/common-errors": "^1.0.1",
    "@types/eslint": "^7.2.6",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.157",
    "@types/module-alias": "^2.0.0",
    "@types/mysql": "^2.15.14",
    "@types/node": "^12.12.47",
    "@types/prettier": "^1.19.1",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sinon": "^9.0.9",
    "@types/source-map-support": "^0.5.1",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/tsc-watch": "^4.2.0",
    "@types/uuid": "^3.4.9",
    "eslint": "^7.15.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-html-reporters": "^2.1.1",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.3",
    "moment": "^2.29.1",
    "prettier": "^2.2.1",
    "sinon": "^9.2.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~4.0.5"
  },
  "dependencies": {
    "@elunic/is-iso-date": "^2.0.0",
    "@elunic/logger": "^4.0.4",
    "@elunic/logger-nestjs": "^1.2.0",
    "@nestjs/common": "^7.6.1",
    "@nestjs/core": "^7.6.1",
    "@nestjs/platform-express": "^7.6.1",
    "@nestjs/swagger": "^4.7.6",
    "@nestjs/typeorm": "^7.1.5",
    "common-errors": "^1.2.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "http-errors": "^1.8.0",
    "joi": "^17.3.0",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "mysql": "^2.18.1",
    "nestjs-joi": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "source-map-support": "^0.5.19",
    "swagger-ui-express": "^4.1.5",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^1.1.0",
    "uuid": "^7.0.3"
  }
}
